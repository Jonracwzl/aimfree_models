{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "allOf": [
        {
            "$ref": "./model.json#"
        },
        {
            "properties": {
                "header": {
                    "properties": {
                        "style": {
                            "const": "parameter"
                        },
                        "id": {
                            "pattern": "^PAR-PRP-[0-9]{3}$"
                        }
                    }
                },
                "body": {
                    "properties": {
                        "value": {
                            "pattern": "^COM-PAT-[0-9]{3}$"
                        },
                        "dimension": {
                            "const": "None"
                        },
                        "range": {
                            "const": "None"
                        },
                        "datatype": {
                            "const": "string"
                        },
                        "unit": {
                            "const": "None"
                        }
                    },
                    "additionalProperties": false
                }
            }
        }
    ]
}