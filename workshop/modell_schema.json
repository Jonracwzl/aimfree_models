{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"id": {
			"$ref": "#/definitions/id"
		},
		"revision": {
			"$ref": "#/definitions/revision"
		},
		"name": {
			"$ref": "#/definitions/name"
		},
		"description": {
			"type": "string"
		}
	},
	"required": ["id","revision"
	],
	"definitions": {	
	"name": {
			"type": "string",
			"pattern": "^[A-Za-z_][A-Za-z0-9_]*$",
			"minLength": 3
		},
		"revision": {
			"type": "number"

			},
		"description": {
			"type": "string"
			},
			"id": {
				"type": "string",
				"pattern": "^[A-Za-z_][A-Za-z0-9_]*$",
				"minLength": 3
				}
	}
}